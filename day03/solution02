public class Main {
    public static void main(String[] args) {
        // Скопируй код себе в IntelliJIDEA и продолжи решение
        String result = concat("Orange", "Bubbles");
        System.out.println(result);
    }

    public static String concat(String s1, String s2) {
        // Допиши метод тут ...
        return s2 + s1 + s2;
    }
}
